{
  "name": "qa-automation-challenge",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "setup": "npm install && npm run install-client && npm run install-server",
    "install-client": "cd client && npm install",
    "install-server": "cd server && npm install",
    "start": "npm run setup && concurrently \"npm run server\" \"npm run client\"",
    "server": "node server/start-server.js",
    "client": "node client/start-client.js"
  },
  "devDependencies": {
    "concurrently": "4.1.1",
    "cypress": "^13.15.0"
  }
}
